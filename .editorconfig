# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = false
max_line_length = 80
trim_trailing_whitespace = true
ij_typescript_use_double_quotes = false;

# Markdown override allow for spaces at end of line
[*.md]
trim_trailing_whitespace = false
